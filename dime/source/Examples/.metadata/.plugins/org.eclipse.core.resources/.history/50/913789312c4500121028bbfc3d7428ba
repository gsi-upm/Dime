<!-- Declaration of the global statements -->


<var name="times_start_dime" expr="0"/>

<var name="DIGITS"/>

<var name="RECORD"/>

<var name="LASTSTATE" expr="&quot;start&quot;"/>

<!-- Declaration of the states -->

<!-- Declaration of the state start -->
<form id="start">
	<block>
		<assign name="LASTSTATE" expr="&quot;start&quot;" />
	</block>
	<if cond="times_start_dime==0">
		<throw event="times_start_event"/> 
	</if>
	<!-- If structure declaration -->
	
	<!--  IF cond=(true) -->
	<block cond="(true)">
		<prompt>SendData application.</prompt>
	</block>
	<!-- Send implementation for HTTP GET with cURL -->
	
	<block cond="(true)">
		<var name="response_dime" /> 
		<data name="response_dime" method="get" srcexpr="&quot;get.php&quot;+&quot;?&quot;+&quot;caller=&quot;+escape(session.callerid)" />
	</block>
	 
	<!-- End Send implementation--> 
	<block cond="(true)">
		<disconnect/>
	</block>
	<!-- If structure declaration -->
	
	<!--  IF cond=(true) -->
	<block cond="(!((true)))&amp;&amp;(true)">
		<prompt>SendData application.</prompt>
	</block>
	<!-- Send implementation for HTTP GET with cURL -->
	
	<block cond="(!((true)))&amp;&amp;(true)">
		<var name="response_dime" /> 
		<data name="response_dime" method="get" srcexpr="&quot;get.php&quot;+&quot;?&quot;+&quot;caller=&quot;+escape(session.callerid)" />
	</block>
	 
	<!-- End Send implementation--> 
	<block cond="(!((true)))&amp;&amp;(true)">
		<disconnect/>
	</block>
	
	<!-- ENDIF -->
	<!-- ELSE  IF cond=(false) -->
	<block cond="(!((!((true)))&amp;&amp;(true)))&amp;&amp;(false)">
		<prompt>SendData application.</prompt>
	</block>
	
	<!-- ENDIF -->
	<!--ELSE -->
	<block cond="(!((!((true)))&amp;&amp;(true)))&amp;&amp;(false)">
		<prompt>SendData application.</prompt>
	</block>
	<!-- End of the if declaration -->
	
	<!-- ENDIF -->
	<!-- ELSE  IF cond=(false) -->
	<block cond="(!((true)))&amp;&amp;(false)">
		<prompt>SendData application.</prompt>
	</block>
	
	<!-- ENDIF -->
	<!--ELSE -->
	<block cond="(!((true)))&amp;&amp;(false)">
		<prompt>SendData application.</prompt>
	</block>
	<!-- End of the if declaration -->
	<block cond="(true)">
		<disconnect/>
	</block>

</form>
	
<!-- End of the state start -->


</vxml>